<component name="ProjectRunConfigurationManager">
    <configuration default="false" name="Main (dynamic-local)" type="SpringBootApplicationConfigurationType"
                   factoryName="Spring Boot">
        <module name="app.main"/>
        <option name="SPRING_BOOT_MAIN_CLASS" value="app.main.Main"/>
        <option name="ACTIVE_PROFILES" value="dev, dynamic-local"/>
        <option name="WORKING_DIRECTORY" value="file://$PROJECT_DIR$/working-directory"/>
        <option name="ALTERNATIVE_JRE_PATH"/>
        <method v="2">
            <option name="Make" enabled="true"/>
        </method>
    </configuration>
</component>